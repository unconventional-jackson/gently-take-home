{
  "name": "@unconventional-jackson/gently-backend-service",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "lint:check:eslint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:check:prettier": "prettier --check . --no-error-on-unmatched-pattern",
    "lint:check:tsc": "tsc --noEmit",
    "lint:fix:eslint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:fix:prettier": "prettier --write . --no-error-on-unmatched-pattern",
    "start:local": "LOGGING_CONSOLE_ENABLED=true ENV=local CLIENT=gently SERVICE=gently-backend-service ts-node-dev src/index.ts",
    "start:dev": "LOGGING_CONSOLE_ENABLED=true ENV=dev CLIENT=gently SERVICE=gently-backend-service node .",
    "start:prod": "LOGGING_CONSOLE_ENABLED=true ENV=prod CLIENT=gently SERVICE=gently-backend-service node .",
    "test:unit": "jest --detectOpenHandles --verbose",
    "test:unit:coverage": "jest --detectOpenHandles --verbose --coverage",
    "test:unit:watch": "jest --detectOpenHandles --verbose --watch",
    "postinstall": "patch-package"
  },
  "author": "Unconventional Code",
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.568.0",
    "@aws-sdk/client-ses": "^3.679.0",
    "@openapitools/openapi-generator-cli": "^2.9.0",
    "@sendgrid/mail": "^8.1.4",
    "@sequelize/core": "^7.0.0-alpha.41",
    "@sequelize/mssql": "^7.0.0-alpha.41",
    "@sequelize/postgres": "^7.0.0-alpha.43",
    "@unconventional-code/observability-sdk": "^0.0.38",
    "@unconventional-jackson/gently-common-service": "^0.0.2",
    "@unconventional-jackson/gently-database-service": "^1.0.9",
    "@unconventional-jackson/gently-openapi-service": "^0.0.7",
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "cron": "^3.1.6",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "didyoumean": "^1.2.2",
    "express": "^4.13.4",
    "express-async-handler": "^1.2.0",
    "form-data": "^4.0.0",
    "jsonwebtoken": "^9.0.2",
    "luxon": "^3.4.4",
    "morgan": "^1.10.0",
    "ms": "^2.1.3",
    "mssql": "^11.0.1",
    "node-cache": "^5.1.2",
    "object-hash": "^3.0.0",
    "objects-to-csv": "^1.3.6",
    "patch-package": "^8.0.0",
    "pdf-lib": "^1.17.1",
    "speakeasy": "^2.0.0",
    "uuid": "^9.0.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@shelf/jest-postgres": "^1.2.1",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/didyoumean": "^1.2.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/luxon": "^3.4.2",
    "@types/morgan": "^1.9.9",
    "@types/ms": "^0.7.34",
    "@types/mssql": "^9.1.5",
    "@types/node": "^20.17.1",
    "@types/object-hash": "^3.0.6",
    "@types/objects-to-csv": "^1.3.3",
    "@types/speakeasy": "^2.0.10",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "@unconventional-code/eslint-config": "^0.0.16",
    "@unconventional-code/prettier-config": "^0.0.3",
    "eslint-import-resolver-typescript": "^3.6.1",
    "jest": "^29.7.0",
    "prettier": "^3.2.4",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "type-fest": "^4.10.2",
    "typescript": "^5.3.3"
  },
  "eslintConfig": {
    "extends": "@unconventional-code/eslint-config",
    "ignorePatterns": [
      "generated",
      "dist"
    ]
  },
  "prettier": "@unconventional-code/prettier-config"
}
