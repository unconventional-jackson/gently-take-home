{
  "name": "@unconventional-jackson/gently-frontend-service",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build:dev": "tsc && vite build --mode dev --debug",
    "build:prod": "tsc && vite build --mode prod --debug",
    "postinstall": "patch-package",
    "start:local": "vite --host --port 80",
    "start:dev": "vite --host --port 80 --mode dev",
    "start:prod": "vite --host --port 80 --mode prod",
    "lint:check:tsc": "tsc --noEmit",
    "lint:check:eslint": "eslint . --ext .js,.jsx,.ts,.tsx --ignore-pattern vite.config.ts",
    "lint:check:prettier": "prettier --check ./**/*.{ts,js,tsx,jsx,graphql,yaml,yml} --no-error-on-unmatched-pattern",
    "lint:fix:prettier": "prettier --write . --no-error-on-unmatched-pattern",
    "test:unit": "echo \"Not implemented for this service\" && exit 0"
  },
